{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:jest/recommended"
    ],
    "plugins": [
        "jest",
        "lodash"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/extensions": "off",
        "no-prototype-builtins": "off",
        "no-underscore-dangle": [
            2,
            {
                "allow": [
                    "__filename",
                    "__dirname"
                ]
            }
        ]
    }
}
